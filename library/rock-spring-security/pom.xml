<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>az.rock</groupId>
        <artifactId>flyjob</artifactId>
        <version>1.0-SNAPSHOT</version>
        <relativePath/>
    </parent>

    <name>FlyJob</name>
    <description>Job search platform</description>

    <licenses>
        <license>
            <name>Rock General Public License v3.0</name>
            <url>https://www.github.com/l00pss/licenses</url>
        </license>
    </licenses>

    <organization>
        <name>FlyJob</name>
    </organization>

    <developers>
        <developer>
            <name>Vugar Mammadli</name>
            <email>mammadli.vugar@hotmail.com</email>
            <organization>FlyJob</organization>
            <organizationUrl>https://www.github.com/flyjob</organizationUrl>
        </developer>
    </developers>

    <groupId>az.rock.lib.spring</groupId>
    <artifactId>rock-spring-security</artifactId>

    <properties>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <spring-boot-starter.version>3.0.4</spring-boot-starter.version>
        <jaxb.version>2.3.1</jaxb.version>
        <jwt.version>0.9.1</jwt.version>
        <slf4j.version>2.0.7</slf4j.version>
        <spring-doc.version>1.6.15</spring-doc.version>
    </properties>


    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-security</artifactId>
            <version>${spring-boot-starter.version}</version>
        </dependency>
        <dependency>
            <groupId>io.jsonwebtoken</groupId>
            <artifactId>jjwt</artifactId>
            <version>${jwt.version}</version>
        </dependency>
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-api</artifactId>
            <version>${slf4j.version}</version>
        </dependency>
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-simple</artifactId>
            <version>${slf4j.version}</version>
        </dependency>
    </dependencies>
</project>